{
  "format": "page",
  "id": "event",
  "label": "Event",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "event_title": {
        "type": "StructuredText",
        "config": {
          "label": "Event Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "strong,em"
        }
      },
      "program": {
        "type": "Select",
        "config": {
          "label": "Program",
          "placeholder": "Choose the program that best describes this event.",
          "options": [
            "Exhibition",
            "Performance",
            "Revolve Sound",
            "Film Screening",
            "Queer Artist Meetup",
            "Skillshare",
            "Community Event"
          ]
        }
      },
      "date_and_time": {
        "type": "Timestamp",
        "config": {
          "label": "Date and Time",
          "placeholder": ""
        }
      },
      "featured_image": {
        "type": "Image",
        "config": {
          "label": "Featured Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "image": {
              "type": "SharedSlice"
            },
            "text_with_image": {
              "type": "SharedSlice"
            },
            "text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}
